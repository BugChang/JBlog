<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Responsive blog template">
    <meta name="author" content="Zoyothemes">
    <link rel="shortcut icon" href="~/images/favicon.ico">
    <title>BugChang's Blog - @ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/css?family=Nunito+Sans:300,600,700" rel="stylesheet">
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/lib/metisMenu/metisMenu.min.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/materialdesign-webfont/css/materialdesignicons.min.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" type="text/css" />
    <link href="~/css/default.css" rel="stylesheet" type="text/css" />
    @await RenderSectionAsync("css",false)
</head>
<body>
    <div id="wrapper">
        @await Component.InvokeAsync("LeftMenu")
        <div class="page-wrapper">
            @RenderBody()
        </div>
    </div>
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/lib/metismenu/metismenu.min.js"></script>
    <script src="~/js/jquery.app.js"></script>
    @await RenderSectionAsync("scripts",false)
</body>
</html>